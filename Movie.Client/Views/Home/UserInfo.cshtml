@using UserInfoViewModel = Movie.Client.Controllers.UserInfoViewModel
@using Microsoft.AspNetCore.Mvc.TagHelpers
@model UserInfoViewModel

@{
    ViewData["Title"] = "UserInfo";
}

<h1>UserInfo</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model._dictionary) {
        <tr>
            <td>
                @Html.DisplayText( item.Key ) |
                @Html.DisplayText( item.Value )
            </td>
        </tr>
}
    </tbody>
</table>
